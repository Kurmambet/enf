    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/enf/manage.py",
            "args": [
                "runserver",
                "8000"
            ],
            "django": true
        }
    ]
}


